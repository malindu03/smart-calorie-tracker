# Example environment variables for the Calorie Tracker application
# Copy this file to .env and fill in the actual values before running the server

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/calorie_tracker?schema=public"

# JWT Secret
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# Server
PORT=5000
NODE_ENV=development

# API Keys
GEMINI_API_KEY=""
SPOONACULAR_API_KEY=""
